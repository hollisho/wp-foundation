{
    "name": "hollisho/wp-foundation",
    "description": "Modern WordPress plugin/theme foundation framework with DI container and service providers",
    "type": "library",
    "keywords": ["wordpress", "framework", "dependency-injection", "service-provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hollisho",
            "email": "hollisho@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "php-di/php-di": "^6.4",
        "psr/container": "^1.0|^2.0",
        "hollisho/wp-orm": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "WPFoundation\\": "src/"
        },
        "files": [
            "src/Support/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
